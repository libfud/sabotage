[build]
#patch -p1 < "$K"/some.patch || exit 1

./configure -C --prefix="$butch_prefix" || exit 1
CFLAGS="$optcflags" LDFLAGS="$optldflags" \
make -j$MAKE_THREADS || exit 1
#This is in the dtach README, you don't "install" it in the typical sense
install -D dtach $butch_install_dir/bin/dtach || exit 1
install -D dtach.1 $butch_install_dir/share/man/man1/dtach.1 || exit 1
